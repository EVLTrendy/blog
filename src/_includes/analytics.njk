<!-- Google Analytics 4 -->
<script async src="https://www.googletagmanager.com/gtag/js?id={{ settings.analytics.googleAnalytics }}"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', '{{ settings.analytics.googleAnalytics }}');
</script>
